{
  "functions": {
    "api/index.py": {
      "includeFiles": "backend/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/models",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/models/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/research/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/events/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/config",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/health",
      "destination": "/api/index.py"
    }
  ]
} 